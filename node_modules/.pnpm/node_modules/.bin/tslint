#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/NodeJs/node_modules/.pnpm/tslint@5.20.1_typescript@5.8.2/node_modules/tslint/bin/node_modules:/mnt/d/NodeJs/node_modules/.pnpm/tslint@5.20.1_typescript@5.8.2/node_modules/tslint/node_modules:/mnt/d/NodeJs/node_modules/.pnpm/tslint@5.20.1_typescript@5.8.2/node_modules:/mnt/d/NodeJs/node_modules/.pnpm/node_modules:/mnt/d/NodeJs/node_modules:/mnt/d/node_modules:/mnt/d/Nodejs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/NodeJs/node_modules/.pnpm/tslint@5.20.1_typescript@5.8.2/node_modules/tslint/bin/node_modules:/mnt/d/NodeJs/node_modules/.pnpm/tslint@5.20.1_typescript@5.8.2/node_modules/tslint/node_modules:/mnt/d/NodeJs/node_modules/.pnpm/tslint@5.20.1_typescript@5.8.2/node_modules:/mnt/d/NodeJs/node_modules/.pnpm/node_modules:/mnt/d/NodeJs/node_modules:/mnt/d/node_modules:/mnt/d/Nodejs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tslint/bin/tslint" "$@"
else
  exec node  "$basedir/../tslint/bin/tslint" "$@"
fi
